# GitHub API Token with repo access permissions
# Generate from: https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here

# OpenAI API Key
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Tavily API Key
# Get from: https://tavily.com/api-keys
TAVILY_API_KEY=your_tavily_api_key_here

# Optional: Logging Configuration
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FILE=awesome-embodied-ai.log

# Optional: GitHub Configuration
GITHUB_USERNAME=your_github_username
GITHUB_EMAIL=your_github_email

# Optional: OpenAI Configuration
OPENAI_ORG_ID=your_openai_org_id  # If using organization-specific API access
OPENAI_MODEL=gpt-4  # Override default model from config.yaml 

# Twitter API Keys
TWITTER_API_KEY=your_api_key
TWITTER_API_SECRET=your_api_secret
TWITTER_ACCESS_TOKEN=your_access_token
TWITTER_ACCESS_TOKEN_SECRET=your_access_token_secret